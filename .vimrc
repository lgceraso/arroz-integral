set nocompatible
syntax on
set number
set spelllang=en
set encoding=utf-8
set showcmd
set nowrap
filetype plugin indent on
set tabstop=4 shiftwidth=4
set expandtab
set backspace=indent,eol,start
set incsearch
set ignorecase
set smartcase
set autoindent
set copyindent
set smarttab
set nobackup
set noswapfile
:let mapleader = " "
nnoremap <leader><leader> :x <cr>
